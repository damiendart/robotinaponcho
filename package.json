{
  "name": "robotinaponcho",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "buildstatic": "./buildstatic"
  },
  "files": [
    "buildstatic"
  ],
  "dependencies": {
    "autoprefixer": "*",
    "clean-css-cli": "*",
    "html-minifier": "*",
    "postcss-cli": "*",
    "sass": "*",
    "svgo": "*",
    "uglify-js": "*"
  },
  "devDependencies": {
    "assets": "git+ssh://git@github.com/damiendart/assets.git#7eb507007b3880d77d4f638edd510bc1966ef30a",
    "toolbox-sass": "git+https://www.robotinaponcho.net/git/toolbox-sass.git#e27c5b8269d6d7bc776c26d656878e5b40997f7e",
    "matter-js": "*",
    "poly-decomp": "0.2.1"
  },
  "scripts": {
    "assets": "cp -r ./node_modules/assets/www.robotinaponcho.net/* static_src",
    "prelastupdated": "git config filter.lastupdated.clean \"./bin/lastupdated\" && git config filter.lastupdated.smudge \"./bin/lastupdated %f\"", 
    "lastupdated": "touch static_src/notes/*.markdown && git checkout static_src/notes/*.markdown"
  }
}

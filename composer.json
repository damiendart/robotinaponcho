{
  "authors": [
    {
      "email": "damiendart@pobox.com",
      "homepage": "https://www.robotinaponcho.net",
      "name": "Damien Dart",
      "role": "Developer"
    }
  ],
  "autoload": {
    "files": ["protected/helpers.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/",
      "StaticSiteGenerator\\": "src/ssg/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "vendor-dir": "protected/vendor",
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "description": "Source code to Damien Dart's personal site.",
  "license": "MIT",
  "name": "damiendart/robotinaponcho",
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-intl-grapheme": "*",
    "symfony/polyfill-intl-normalizer": "*",
    "symfony/polyfill-mbstring": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "require": {
    "php": "8.2.*",
    "ext-ctype": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "filp/whoops": "^2.18.1",
    "guzzlehttp/guzzle": "^7.9.3",
    "sentry/sentry": "^4.11.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "friendsofphp/php-cs-fixer": "^3.75.0",
    "league/commonmark": "^2.7.0",
    "michelf/php-smartypants": "^1.8.1",
    "phpstan/phpstan": "^2.1.17",
    "phpstan/phpstan-strict-rules": "^2.0.4",
    "phpunit/phpunit": "^11.5.21",
    "psalm/phar": "^6.12.0",
    "roave/security-advisories": "dev-latest",
    "symfony/yaml": "^7.3.0",
    "twig/twig": "^3.21.1",
    "voku/html-min": "^4.5.1"
  },
  "type": "project"
}
